{
  "private": true,
  "name": "root",
  "workspaces": [
    "packages/*"
  ],
  "license": "UNLICENSED",
  "scripts": {
    "clean": "npx lerna clean -y && rm -rf node_modules",
    "build": "npx lerna run build --concurrency 2",
    "reinstall": "npx yarn clean && yarn && npx lerna exec yarn",
    "start": "npx nodemon --watch ./**/src -e '.ts' --exec 'npx lerna run build'",
    "prerelease": "npx lerna publish prerelease -y"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "4.30.0",
    "@typescript-eslint/parser": "4.30.0",
    "eslint": "7.32.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "lerna": "4.0.0",
    "nodemon": "2.0.12",
    "typescript": "4.4.3"
  },
  "resolutions": {
    "@google-cloud/pubsub": "2.17.0",
    "@google-cloud/storage": "5.3.0",
    "@types/node": "12.20.25",
    "@sentry/minimal": "6.5.1",
    "@sentry/node": "6.5.1",
    "atlassian-jwt": "2.0.2",
    "axios": "0.21.1",
    "cookie-parser": "1.4.4",
    "express": "4.17.0",
    "firebase": "9.0.1",
    "firebase-admin": "9.9.0",
    "firebase-functions": "3.14.1",
    "inversify": "5.1.1",
    "inversify-express-utils": "6.3.2",
    "inversify-inject-decorators": "3.1.0",
    "passport": "0.4.1",
    "passport-http-bearer": "1.0.1",
    "passport-jwt": "4.0.0",
    "path-to-regexp": "6.1.0",
    "redis": "3.1.2",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "styled-components": "3.2.6",
    "typescript": "4.4.3",
    "uniqid": "5.4.0"
  }
}
