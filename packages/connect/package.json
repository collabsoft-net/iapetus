{
  "name": "@collabsoft-net/connect",
  "version": "1.0.0-alpha.157",
  "description": "Atlassian Connect polyfill (incl. Javascript API)",
  "author": "CollabSoft <info@collabsoft.net>",
  "homepage": "https://github.com/collabsoft-net/iapetus#readme",
  "license": "UNLICENSED",
  "sideEffects": false,
  "module": "./dist/es2020/index.js",
  "main": "./dist/cjs/index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist",
    "typings"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/collabsoft-net/iapetus.git"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "lint": "npx eslint . -c ../../.eslintrc.js --ignore-path ../../.eslintignore --ext .ts,.tsx",
    "build": "npx yarn clean && npx yarn lint && npx yarn build:cjs && npx yarn build:es2020 && npx yarn build:types",
    "build:cjs": "npx tsc --module commonjs --outDir dist/cjs",
    "build:es2020": "npx tsc --module es2020 --outDir dist/es2020",
    "build:types": "npx tsc --emitDeclarationOnly --outDir dist/types",
    "start": "npx nodemon --watch src -e '.ts' --exec 'npx yarn build'",
    "prepack": "npx yarn build",
    "preinstall": "echo Installing dependencies for @collabsoft-net/connect"
  },
  "bugs": {
    "url": "https://github.com/collabsoft-net/iapetus/issues"
  },
  "devDependencies": {
    "@atlaskit/modal-dialog": "12.0.1",
    "@collabsoft-net/types": "^1.0.0-alpha.157",
    "@types/node": "12.20.25",
    "@types/react": "16.9.54",
    "@types/react-dom": "16.9.9",
    "inversify": "5.1.1",
    "query-string": "^7.0.1",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "styled-components": "3.2.6"
  },
  "peerDependencies": {
    "@atlaskit/modal-dialog": "12.0.1",
    "inversify": "5.1.1",
    "query-string": "^7.0.1",
    "react": "16.14.0",
    "react-dom": "16.14.0",
    "styled-components": "3.2.6"
  },
  "gitHead": "90588ca94d4036648c49a76557e532e385259578",
  "dependencies": {
    "@juggle/resize-observer": "^3.3.1"
  }
}
