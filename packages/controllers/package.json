{
  "name": "@collabsoft-net/controllers",
  "version": "1.0.0-alpha.144",
  "description": "Shared controllers for Firebase Cloud Functions",
  "author": "CollabSoft <info@collabsoft.net>",
  "homepage": "https://github.com/collabsoft-net/iapetus#readme",
  "license": "UNLICENSED",
  "sideEffects": false,
  "module": "./dist/es2020/index.js",
  "main": "./dist/cjs/index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist",
    "typings"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/collabsoft-net/iapetus.git"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "lint": "npx eslint . -c ../../.eslintrc.js --ignore-path ../../.eslintignore --ext .ts,.tsx",
    "build": "npx yarn clean && npx yarn lint && npx yarn build:cjs && npx yarn build:es2020 && npx yarn build:types",
    "build:cjs": "npx tsc --module commonjs --outDir dist/cjs",
    "build:es2020": "npx tsc --module es2020 --outDir dist/es2020",
    "build:types": "npx tsc --emitDeclarationOnly --outDir dist/types",
    "start": "npx nodemon --watch src -e '.ts' --exec 'npx yarn build'",
    "prepack": "npx yarn build",
    "preinstall": "echo Installing dependencies for @collabsoft-net/controllers"
  },
  "bugs": {
    "url": "https://github.com/collabsoft-net/iapetus/issues"
  },
  "devDependencies": {
    "@collabsoft-net/dto": "^1.0.0-alpha.128",
    "@collabsoft-net/functions": "^1.0.0-alpha.144",
    "@collabsoft-net/services": "^1.0.0-alpha.144",
    "@collabsoft-net/types": "^1.0.0-alpha.128",
    "@sentry/minimal": "6.5.1",
    "@sentry/node": "6.5.1",
    "@types/passport": "1.0.7",
    "inversify": "5.1.1",
    "inversify-express-utils": "6.3.2",
    "passport": "0.4.1"
  },
  "peerDependencies": {
    "@collabsoft-net/dto": "1.0.0-alpha.44",
    "@collabsoft-net/functions": "1.0.0-alpha.45",
    "@collabsoft-net/services": "1.0.0-alpha.45",
    "@sentry/minimal": "6.5.1",
    "@sentry/node": "6.5.1",
    "inversify": "5.1.1",
    "inversify-express-utils": "6.3.2",
    "passport": "0.4.1"
  },
  "gitHead": "90588ca94d4036648c49a76557e532e385259578"
}
