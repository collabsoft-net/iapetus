    "@gdn/envify-nconf": "1.2.1",
    "@sentry/minimal": "6.5.1",
    "@sentry/node": "6.5.1",
    "@types/cookie-parser": "1.4.2",
    "cookie-parser": "1.4.4",
    "express": "4.17.0",
    "firebase-admin": "9.9.0",
    "firebase-functions": "3.14.1",
    "inversify": "5.1.1",
    "inversify-express-utils": "6.3.2",
    "passport": "0.4.1"
  },
  "peerDependencies": {
