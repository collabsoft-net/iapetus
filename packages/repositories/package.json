{
  "name": "@iapetus/repositories",
  "version": "0.0.0",
  "description": "Implementations of supported repositories",
  "author": "CollabSoft <info@collabsoft.net>",
  "homepage": "https://github.com/collabsoft-net/iapetus#readme",
  "license": "UNLICENSED",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/collabsoft-net/iapetus.git"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "lint": "npx eslint . --ext .ts,.tsx",
    "build": "npx yarn clean && npx yarn lint && npx tsc",
    "start": "npx nodemon --watch src -e '.ts' --exec 'npx yarn build'",
    "prepack": "npx yarn build"
  },
  "bugs": {
    "url": "https://github.com/collabsoft-net/iapetus/issues"
  },
  "devDependencies": {
    "@firebase/app-types": "0.x",
    "@google-cloud/storage": "^5.14.0",
    "@iapetus/emitters": "^0.0.0",
    "@iapetus/types": "^0.0.0",
    "@types/node": "^16.7.10",
    "@types/uniqid": "^5.3.1",
    "firebase-admin": "^9.11.1",
    "uniqid": "^5.4.0"
  }
}
