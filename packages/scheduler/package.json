{
  "name": "@collabsoft-net/scheduler",
  "version": "1.0.0-alpha.90",
  "description": "Scheduler implementation",
  "author": "CollabSoft <info@collabsoft.net>",
  "homepage": "https://github.com/collabsoft-net/iapetus#readme",
  "license": "UNLICENSED",
  "sideEffects": false,
  "module": "./dist/es2020/index.js",
  "main": "./dist/cjs/index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/collabsoft-net/iapetus.git"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "lint": "npx eslint . -c ../../.eslintrc.js --ignore-path ../../.eslintignore --ext .ts,.tsx",
    "build": "npx yarn clean && npx yarn lint && npx yarn build:cjs && npx yarn build:es2020 && npx yarn build:types",
    "build:cjs": "npx tsc --module commonjs --outDir dist/cjs",
    "build:es2020": "npx tsc --module es2020 --outDir dist/es2020",
    "build:types": "npx tsc --emitDeclarationOnly --outDir dist/types",
    "start": "npx nodemon --watch src -e '.ts' --exec 'npx yarn build'",
    "prepack": "npx yarn build"
  },
  "bugs": {
    "url": "https://github.com/collabsoft-net/iapetus/issues"
  },
  "devDependencies": {
    "@collabsoft-net/types": "^1.0.0-alpha.85",
    "@google-cloud/scheduler": "2.3.0"
  },
  "peerDependencies": {
    "@google-cloud/scheduler": "2.3.0"
  }
}
