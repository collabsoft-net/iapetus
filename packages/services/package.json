{
  "name": "@collabsoft-net/services",
  "version": "1.0.0-alpha.108",
  "description": "Abstract services to be used for app-specific implementations",
  "author": "CollabSoft <info@collabsoft.net>",
  "homepage": "https://github.com/collabsoft-net/iapetus#readme",
  "license": "UNLICENSED",
  "sideEffects": false,
  "module": "./dist/es2020/index.js",
  "main": "./dist/cjs/index.js",
  "types": "./dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/collabsoft-net/iapetus.git"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "lint": "npx eslint . -c ../../.eslintrc.js --ignore-path ../../.eslintignore --ext .ts,.tsx",
    "build": "npx yarn clean && npx yarn lint && npx yarn build:cjs && npx yarn build:es2020 && npx yarn build:types",
    "build:cjs": "npx tsc --module commonjs --outDir dist/cjs",
    "build:es2020": "npx tsc --module es2020 --outDir dist/es2020",
    "build:types": "npx tsc --emitDeclarationOnly --outDir dist/types",
    "start": "npx nodemon --watch src -e '.ts' --exec 'npx yarn build'",
    "prepack": "npx yarn build",
    "preinstall": "echo Installing dependencies for @collabsoft-net/services"
  },
  "bugs": {
    "url": "https://github.com/collabsoft-net/iapetus/issues"
  },
  "devDependencies": {
    "@collabsoft-net/clients": "^1.0.0-alpha.108",
    "@collabsoft-net/enums": "^1.0.0-alpha.108",
    "@collabsoft-net/helpers": "^1.0.0-alpha.108",
    "@collabsoft-net/types": "^1.0.0-alpha.108",
    "http-status-codes": "^2.1.4",
    "inversify": "5.1.1",
    "path-to-regexp": "6.1.0"
  },
  "peerDependencies": {
    "@collabsoft-net/clients": "1.0.0-alpha.11",
    "@collabsoft-net/enums": "1.0.0-alpha.43",
    "@collabsoft-net/helpers": "1.0.0-alpha.44",
    "http-status-codes": "^2.1.4",
    "inversify": "5.1.1",
    "path-to-regexp": "6.1.0"
  },
  "gitHead": "90588ca94d4036648c49a76557e532e385259578"
}
